---
apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: istio-ingress
  namespace: istio-system
  annotations: {}
spec:
  ingressClassName: nginx
  rules:
  - host: "kubeflow.{{ config_ingress_k8s_domain }}"
    http:
      paths:
      - path: /
        pathType: Prefix
        backend:
          service:
            name: istio-ingressgateway
            port:
              number: 80